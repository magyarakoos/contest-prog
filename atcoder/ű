#include <bits/stdc++.h>
using namespace std;
#define int int64_t

const int INF = 1e18, MAXN = 2e5, MOD = 1e9 + 7;

#define cases 0
void solve() {
    int N;
    cin >> N;
    for (int y = 1; y * y * y < N; y++) {
        double rt = cbrt(N + y * y * y);
        int fl = floor(rt);
        if (rt == fl) {
            cout << fl << " " << y << "\n";
            return;
        }
    }
    cout << "-1\n";
}

int32_t main() {
    cin.tie(0), ios::sync_with_stdio(0);
    int T = 1;
    if (cases) cin >> T;
    while (T--) solve();
}
